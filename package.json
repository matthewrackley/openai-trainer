{
  "name": "openai-trainer",
  "productName": "OpenAI",
  "description": "My Electron application description",
  "keywords": [],
  "main": "./main.js",
  "browser": "./server.js",
  "version": "1.0.5",
  "author": "kali",
  "scripts": {
    "app": "electron .",
    "app:dev": "electron . --dev",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --open",
    "start": "express ./server.js",
    "docs:api": "jsdoc2md --configure tooling/docs/jsdoc.json --files src/**/*.js > docs/api.md",
    "docs:prepare": "gitbook install",
    "docs:build": "npm run docs:prepare && npm run docs:api && gitbook build",
    "docs:serve": "npm run docs:prepare && npm run docs:api && gitbook serve",
    "docs:clean": "rimraf _book"
  },
  "dependencies": {
    "@progress/kendo-react-conversational-ui": "5.12.1-dev.202303210819",
    "@progress/kendo-react-dateinputs": "5.12.1-dev.202303210819",
    "api-ai-javascript": "2.0.0-beta.21",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.2.3",
    "cookie-parser": "^1.4.6",
    "crypto-js": "4.1.1",
    "dotenv": "16.0.3",
    "express": "^4.18.2",
    "jquery": "3.6.4",
    "mariadb": "^3.1.1",
    "multer": "^1.4.5-lts.1",
    "open": "^9.1.0",
    "openai": "3.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io": "4.6.1",
    "window": "^1.0.0"
  },
  "devDependencies": {
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "@babel/eslint-parser": "^7.21.3",
    "@eove/eslint-config-electron": "^1.8.0",
    "electron": "22.0.2",
    "eslint": "^8.37.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0"
  },
  "directories": {
    "lib": "lib",
    "events": "events",
    "api": "api",
    "commands": "commands",
    "classes": "classes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matthewrackley/openai-trainer.git"
  },
  "license": "GPL-3.0-or-later",
  "bugs": {
